---
import Layout from '~/layouts/PageLayout.astro';
import HeroText from '~/components/widgets/HeroText.astro';
import ContactUs from '~/components/widgets/Contact.astro';
import Features2 from '~/components/widgets/Features2.astro';

const metadata = {
  title: 'Контакты',
};
---

<Layout metadata={metadata}>
  <!-- HeroText Widget ******************* -->

  <HeroText tagline="Контакты" title="Давайте общаться!" />

  <ContactUs
    title="Отправьте сообщение сейчас!"
    subtitle='Для получения более быстрых ответов ознакомьтесь с нашим разделом часто задаваемых вопросов. Возможно, вы найдете решение, которое ищете, прямо здесь! Если нет, то наша служба поддержки с радостью вам поможет. '
    inputs={[
      {
        type: 'text',
        name: 'name',
        label: 'Ваше имя',
      },
      {
        type: 'email',
        name: 'email',
        label: 'Email',
      },
    ]}
    textarea={{
      label: 'Сообщение',
    }}
    disclaimer={{
      label:
        'Отправляя сообщение вы соглашаетесь на обработку своих персональных данных',
    }}
    description="Обычно наша служба поддержки отвечает в течении 24 часов"
  />

  <!-- Features2 Widget ************** -->

  <Features2
    title="We are here to help!"
    items={[
      {
        title: 'Общая поддержка',
        description: `Обращайтесь к нам по вопросам, связанным с управлением учетной записью, навигацией по веб-сайту, проблемами оплаты или общими вопросами о функциональности веб-сайта.`,
      },
      {
        title: 'Свяжитесь с отделом продаж',
        description:'Обращайтесь к нам по вопросам о покупках круизов, дополнительной информацией о наших теплоходах, маршрутах, расписании и т.д.',
      },
      {
        title: 'Техническая поддержка',
        description:
          'Обращайтесь к нам, если сталкиваетесь с проблемами в работе сайта',
      },
      {
        title: 'Телефон',
        description: '+7(846)270-4040',
        icon: 'tabler:headset',
      },
      {
        title: 'Email',
        description: 'cruise@sputnik-germes.ru',
        icon: 'tabler:mail',
      },
      {
        title: 'Наш адрес',
        description: 'г. Самара, ул. Ленинская, 166',
        icon: 'tabler:map-pin',
      },
    ]}
  />
  <section>
    <h1 class="zag_title_center">Контакты</h1>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <p class="middle_zagolovok_left black_zagolovok">Самара</p>
        </div>
      </div>
      <div style="padding-bottom: 0px;" class="row row_block">
        <div class="col-md-4 call_block_big">
          <p class="middle_contacti_zagolovok_left black_zagolovok">Центральный офис</p>
          <p class="p_contacti"><i class="fa fa-map-marker hottur_i"></i>ул. Ленинская, 166</p>
          <p class="p_contacti"><i class="fa fa-phone hottur_i"></i><span class="phone_ring_1">8 (846) 22 10 300 -
              Единый </span></p>
          <!--    <p class="p_contacti"><i class="fa fa-phone hottur_i"></i><span class="phone_ring_1">8 (846) 201 40 50 - Приемная </span></p> -->
          <p class="p_contacti"><i class="fa fa-envelope-o hottur_i"></i><a href="mailto:sputnik@sputnik-germes.ru">sputnik@sputnik-germes.ru</a></p>
          <p class="p_contacti"><i class="fa fa-clock-o hottur_i"></i><strong><br>пн-пт: 09.00-18.00, <br>сб:
              10.00-16.00, <br>вс: выходной</strong></p>
        </div>
        <div class="col-md-8">
          <div class="container-fluid">
            <div class="row" id="div_otdeli_samara_centr">
              <div class="col-md-12">
                <p class="middle_contacti_zagolovok_left black_zagolovok" style="margin-bottom: 25px;">Отделы
                  центрального офиса</p>
              </div>
              <div class="col-md-6 call_block">
                <p class="small_zagolovok_left black_zagolovok">Речные круизы</p>
                <p class="p_contacti "><i class="fa fa-envelope-o hottur_i"></i><a href="mailto:cruise@sputnik-germes.ru">cruise@sputnik-germes.ru</a></p>
              </div>
              <div class="col-md-6 call_block">
                <p class="small_zagolovok_left black_zagolovok">Зарубежный туризм</p>
                <p class="p_contacti"><i class="fa fa-envelope-o hottur_i"></i><a href="mailto:fit@sputnik-germes.ru">fit@sputnik-germes.ru</a></p>
              </div>
              <div class="col-md-6 call_block">
                <p class="small_zagolovok_left black_zagolovok">Внутренний туризм</p>
                <p class="p_contacti"><i class="fa fa-envelope-o hottur_i"></i><a href="mailto:russia@sputnik-germes.ru">russia@sputnik-germes.ru</a></p>
              </div>
              <div class="col-md-6 call_block">
                <p class="small_zagolovok_left black_zagolovok">Визовый отдел</p>
                <p class="p_contacti"><i class="fa fa-envelope-o hottur_i"></i><a href="mailto:visa@sputnik-germes.ru">visa@sputnik-germes.ru</a></p>
              </div>
              <div class="col-md-12 call_block">
                <p class="small_zagolovok_left black_zagolovok">Работа с агенствами</p><br>
                <p class="p_contacti"><i class="fa fa-phone hottur_i"></i><span class="phone_ring_1">8 (846) 221 64
                    40</span></p>
                <p class="p_contacti"><i class="fa fa-envelope-o hottur_i"></i><a href="mailto:book@sputnik-germes.ru">book@sputnik-germes.ru</a></p>
              </div>
            </div>
          </div>
        </div>
        <div class="col-md-12">
          <div style=" border-bottom: 1px solid lightgray; margin-top: 25px;"></div>
        </div>
      </div>

      <div class="row row_block">
        <div class="col-md-5">
          <p class="middle_contacti_zagolovok_left black_zagolovok">  </p>

        </div>
