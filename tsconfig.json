{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
   //  "baseUrl": "K:/apps/astro/astrowind",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@ui/*": [
        "./src/components/rcui/*"
      ],
      "@typography/*": [
        "./src/components/typography/*"
      ],
      "@lib/*": [
        "./src/utils/*"
      ],
      "@/lib/*": [
        "./src/utils/*"
      ],
      "@/components/ui/*": [
        "./src/components/rcui/*"
      ],
      "@components/ui/*": [
        "./src/components/rcui/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "react"
  }
}